<body class="login">
    <!-- BEGIN LOGO -->
    <div class="logo">
        <h2 style="color:white">Welcome to <b>The Family Member</b></h2>
    </div>
    <!-- END LOGO -->
    <!-- BEGIN LOGIN -->
    <div class="content" style="width: 460px">
        <!-- BEGIN LOGIN FORM -->




        <div class="row">
            <div class="col-md-12">

                <div class="col-md-3"></div>
                <div class="col-md-9">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>The FamilyMember Id:</b></label>
                                <label class="control-label"><b>@Model.TheFamilyMemberId</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>Name:</b></label>
                                <label class="control-label"><b>@Model.Name</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>Date of Birth:</b></label>
                                <label class="control-label"><b>@Model.DOB</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>Blood Group:</b></label>
                                <label class="control-label"><b>@Model.BloodGroup</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>Address:</b></label>
                                <label class="control-label"><b>@Model.Address</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>Emergency ContactName & No:</b></label>
                                <label class="control-label"><b>@Model.EmergencyContactName_No</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>Medical Allergy:</b></label>
                                <label class="control-label"><b>@Model.MedicalAllergy</b></label>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label"><b>QR Code:</b></label>
                                <label class="control-label"><img src="@Url.Content(Model.QRImage)" alt="Image" /></label>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="modal-footer" style="text-align: center">
            <button type="button" onclick="window.history.go(-1)" class="btn btn-default">Back</button>
        </div>
    </div>
   
